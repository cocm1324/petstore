{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES6",
        "outDir": "dist",
        "baseUrl": "./",
        "moduleResolution": "Node",
        "strict": true,
        "preserveConstEnums": true,
        "noEmit": true,
        "sourceMap": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "paths": {
            
        }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "**/*.spec.ts", "**/*.test.ts"]
}